fw4 -A INPUT -i usb0 -p tcp --syn --dport 2222 -j ACCEPT # SSH
fw4 -A INPUT -i usb0 -p udp --sport bootpc --dport bootps -j ACCEPT # DHCP

# Tor
ipset create tor hash:ip,port -exist
#fw4 -A INPUT -i eth0 -p tcp -m set --match-set tor src,src -j ACCEPT
fw4 -A OUTPUT -o eth0 -p tcp --syn -m set --match-set tor dst,dst -j ACCEPT

fw4 -A INPUT  -i lo -p tcp --syn --dport 9050 -j ACCEPT
fw4 -A OUTPUT -o lo -p tcp --syn --dport 9050 -j ACCEPT
